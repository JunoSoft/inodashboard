{
  "kind": "collectionType",
  "collectionName": "travel_posts",
  "info": {
    "name": "travelPosts"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "tripName": {
      "type": "string",
      "required": true
    },
    "tripLocation": {
      "type": "string",
      "required": true
    },
    "tripCost": {
      "type": "string",
      "required": true
    },
    "tripPoster": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "tripDescription": {
      "type": "richtext",
      "required": true
    },
    "travel_comapy": {
      "model": "travel-comapies"
    }
  }
}
