{
  "kind": "collectionType",
  "collectionName": "hotel_services",
  "info": {
    "name": "hotelServices"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "serviceName": {
      "type": "string",
      "required": true
    },
    "serviceCost": {
      "type": "string",
      "required": true
    },
    "Image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "hotel": {
      "model": "hotels",
      "via": "hotel_services"
    }
  }
}
